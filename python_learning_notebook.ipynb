    "1.返回一个可迭代实例："
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class A(object):\n",
    "    def __init__(self):\n",
    "        self.a = 10\n",
    "    def __iter__(self):\n",
    "        for i in range(5):\n",
    "            yield self.a + i\n",
    "\n",
    "a = A() #a为一个可迭代实例\n",
    "for i in a:\n",
    "    print (i)"
   ]
  }
